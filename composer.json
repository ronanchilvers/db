{
    "name": "ronanchilvers/db",
    "description": "Simple database layer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ronan Chilvers",
            "email": "ronan@thelittledot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ronanchilvers\\Db\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ronanchilvers\\Db\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0",
        "aura/sqlschema": "^2.0",
        "nette/php-generator": "^3.0",
        "ronanchilvers/utility": "^1.0",
        "clancats/hydrahon": "^1.1",
        "symfony/console": "^3.4",
        "symfony/finder": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "bin": [
        "bin/manage-db"
    ],
    "suggests": {},
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ronanchilvers/utility.git"
        }
    ]
